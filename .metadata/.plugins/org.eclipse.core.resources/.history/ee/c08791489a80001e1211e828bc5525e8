package com.doubtsharing.models;

public enum UserGrade {

    GRADE_9("9th Grade"),
    GRADE_10("10th Grade"),
    GRADE_11("11th Grade"),
    GRADE_12("12th Grade");

    private String description;

    UserGrade(String description) {
        this.description = description;
    }

    public String getDescription() {
        return description;
    }

	
}
